    "import yfinance as yf\n",
    "import pandas as pd\n",
    "\n",
    "kospi = yf.download('^KS11', start='2010-01-01', interval='3mo')\n",
    "kospi = kospi['Close']\n",
    "\n",
    "kospi_before = kospi.shift(1)\n",
    "x = pd.DataFrame()\n",
    "x['Close'] = ((kospi / kospi_before) - 1)*100\n",
    "x = x.dropna()\n",
    "\n",
    "grown_result = x['Close'].sum() / 52"
   ]
  }
